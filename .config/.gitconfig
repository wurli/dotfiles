[user]
name = Jacob Scott
email = jscott2718@gmail.com

[core]
editor = nvim
autocrlf = false
excludesFile = '~/.gitignore'

[alias]
co = checkout
ci = commit
st = status
rb = rebase
br = branch
nl = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all

[push]
autoSetupRemote = true

[http]
sslVerify = false

[filter "lfs"]
clean = git-lfs clean -- %f
smudge = git-lfs smudge -- %f
process = git-lfs filter-process
required = true
